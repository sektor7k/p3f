{
  "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST"
        }
      ]
    }
  ]
}
